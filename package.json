{
  "name": "pxl-mongodb",
  "version": "0.0.7",
  "description": "Access counting for any Express-served url - e.g. for a tracking pixel in emails",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/gulp -f gulpfile.js --cwd . dev",
    "test": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/gulp -f gulpfile.js --cwd . ci",
    "test-debug": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/gulp -f gulpfile.js --cwd . testDebug",
    "test-publish": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/gulp -f gulpfile.js --cwd . ci",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/analog-nico/pxl-mongodb.git"
  },
  "keywords": [
    "counting",
    "counter",
    "express",
    "connect",
    "url",
    "uri",
    "endpoint",
    "access",
    "query",
    "querystring",
    "tracking",
    "tracker",
    "pixel",
    "mongodb"
  ],
  "author": "Nicolai Kamenzky (https://github.com/analog-nico)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/analog-nico/pxl-mongodb/issues"
  },
  "homepage": "https://github.com/analog-nico/pxl-mongodb#readme",
  "engines": {
    "node": ">=14.20.1"
  },
  "dependencies": {
    "mongodb": "5.x || 6.x",
    "pxl": "0.0.4"
  },
  "devDependencies": {
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "publish-please": "^5.5.2",
    "uuid": "^9.0.1"
  }
}
